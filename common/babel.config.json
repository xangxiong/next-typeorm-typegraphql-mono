{
  "presets": [
    [
      "next/babel",
      {
        "class-properties": {
          "loose": true
        }
      },
      "gaia-next-babel"
    ]
  ],
  "plugins": [
    "babel-plugin-transform-typescript-metadata",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      },
      "gaia-plugin-proposal-decorators"
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true 
      },
      "gaia-plugin-proposal-class-properties"
    ]
  ]
}